{
  "name": "recipe-flow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun --bun next dev",
    "build": "bun --bun next build",
    "start": "bun --bun next start",
    "lint": "bun biome check . && bun eslint .",
    "lint:fix": "bun biome check --write . && bun eslint . --fix",
    "format": "bun biome check --write .",
    "typecheck": "bun tsc --noEmit",
    "check": "bun run lint && bun run typecheck",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^2.0.76",
    "ai": "^6.0.6",
    "ai-sdk-provider-claude-code": "^3.1.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/bun": "^1.3.5",
    "@types/jsdom": "^27.0.0",
    "@types/jszip": "^3.4.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "canvas": "^3.2.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.1",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  }
}
